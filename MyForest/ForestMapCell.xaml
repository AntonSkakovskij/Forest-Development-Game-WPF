<UserControl x:Class="MyForest.ForestMapCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:MyForest"
             mc:Ignorable="d"
             Background="Transparent">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Canvas Name="Canvas">
            <Rectangle Name="CellBody" Stroke ="Black" StrokeThickness="0" Fill="Green"  Canvas.Top ="0" Canvas.Left="0"/>
            <!--<Image Name="CellBodyGras" Source=""  HorizontalAlignment="Center" VerticalAlignment="Center" ></Image>-->
            <Image Name="CellBodyImage" Source=""  HorizontalAlignment="Center" VerticalAlignment="Center" ></Image>
        </Canvas>
    </Grid>
    

</UserControl>
